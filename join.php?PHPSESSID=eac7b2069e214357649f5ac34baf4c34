<SCRIPT language="javascript">
function popup(win_name, url, iW, iH)
{
	// screen size
	scrH = self.screen.height;
	scrW = self.screen.width;
	pLeft = (scrW - iW)/2;
	pTop = (scrH - iH)/2;

	var popup = window.open (url, win_name, "height="+iH+",width="+iW+",top="+pTop+",left="+pLeft+",resizable=yes,status=no,toolbar=no,menubar=no,location=no, scrollbars=yes");
	if (window.focus) 
	{
		popup.focus()
	}
	
	return false;
}
</SCRIPT>

